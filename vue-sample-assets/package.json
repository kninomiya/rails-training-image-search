{
  "repository": {},
  "license": "MIT",
  "description": "training assets",
  "scripts": {
    "build-css": "node-sass --include-path scss scss/app.scss css/app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "deploy": "npm run build:production",
    "build": "npm run build-css && webpack --mode development",
    "build:production": "npm run build-css && webpack --mode production",
    "watch": "webpack --mode development -w",
    "lint:fix": "tslint --project . --fix",
    "lint": "tslint --project ."
  },
  "dependencies": {},
  "devDependencies": {
    "node-sass": "*",
    "@types/node": "*",
    "axios": "*",
    "babel-brunch": "^6.1.1",
    "brunch": "^2.10.16",
    "clean-css-brunch": "2.10.0",
    "css-loader": "*",
    "copy-webpack-plugin": "*",
    "html-loader": "*",
    "ts-loader": "^4.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "*",
    "typescript": "^2.7.2",
    "uglify-js-brunch": "2.10.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-loader": "^14.2.2",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-property-decorator": "*",
    "vue-template-compiler": "*",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.0.13",
    "moment": "*"
  }
}
